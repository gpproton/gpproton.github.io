{
  "name": "personal-site",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "build:github": "nuxt build --preset github_pages",
    "dev": "nuxt dev",
    "serve": "pnpx serve .output/public",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "clean": "pnpx rimraf node_modules **/node_modules **/dist/* pnpm-lock.*",
    "generate-assets": "pwa-assets-generator",
    "lint:js": "eslint --ext \".ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "pnpm lint:js && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix"
  },
  "devDependencies": {
    "@headlessui/vue": "^1.7.19",
    "@heroicons/vue": "^2.1.3",
    "@nuxt/content": "^2.12.1",
    "@nuxt/eslint-config": "^0.3.0",
    "@nuxtjs/i18n": "^8.1.1",
    "@nuxtjs/tailwindcss": "^6.11.4",
    "@pinia/nuxt": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vite-pwa/nuxt": "^0.6.0",
    "@vueuse/nuxt": "^10.6.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nuxt": "^3.11.2",
    "nuxt-primevue": "^0.3.1",
    "prettier": "^3.2.5",
    "primeicons": "^6.0.1",
    "sass": "^1.69.5",
    "typescript": "^5.4.4"
  },
  "dependencies": {
    "nuxt-content-git": "^4.0.0",
    "primevue": "^3.49.1",
    "vue": "^3.4.19",
    "vue-router": "^4.3.0"
  }
}
